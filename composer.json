{
    "name": "felixkiss/multiple-validators",
    "description": "Remove incompatibilities between multiple Laravel validator packages.",
    "keywords": ["laravel", "validator"],
    "homepage": "https://github.com/felixkiss/multiple-validators",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Kiss",
            "email": "felix@felixkiss.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.x",
        "illuminate/validation": "5.x"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "minimum-stability": "stable"
}
